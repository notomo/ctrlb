{
    "name": "ctrlb",
    "description": "Control browser",
    "version": "0.0.1",
    "manifest_version": 2,
    "icons": {
        "16": "images/icon-16.png",
        "128": "images/icon-128.png"
    },
    "background": {
        "scripts": [
            "scripts/background.js"
        ]
    },
    "options_page": "pages/options.html",
    "browser_action": {
        "default_icon": {
            "19": "images/icon-19.png",
            "38": "images/icon-38.png"
        }
    },
    "permissions": [
        "tabs",
        "storage"
    ]
}
